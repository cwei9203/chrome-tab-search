{
  "manifest_version": 3,
  "name": "Tab Search",
  "version": "1.0",
  "description": "快速搜索并切换标签页、书签和历史记录",
  "permissions": [
    "tabs",
    "bookmarks",
    "history",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.svg",
      "48": "images/icon48.svg",
      "128": "images/icon128.svg"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html"
}